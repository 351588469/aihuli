<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="GMBResultMapper">
	
	<!--表名 -->
	<sql id="tableName">
		GM_GMBRESULT
	</sql>
	
	<!-- 字段 -->
	<sql id="Field">
		GMBR_GMB_ID,	
		GMBR_GMB_FLOOR,	
		GMBR_GMB_LAYER,	
		GMBR_GMB_ROOM,	
		GMBR_ENVIRONMENT,	
		GMBR_SANITATION,	
		GMBR_SAFE,	
		GMBR_EQUIPMENT,	
		GMBR_CPHOTO,	
		GMBR_PHOTOA,	
		GMBR_PHOTOB,	
		GMBR_PHOTOC,	
		GMBR_PHOTOD,	
		GMBR_DESC,	
		GMBR_CTIME,	
		GMBR_UTIME,	
		GMBR_GMU_ID,	
		GMBRESULT_ID
	</sql>
	
	<!-- 字段值 -->
	<sql id="FieldValue">
		#{GMBR_GMB_ID},	
		#{GMBR_GMB_FLOOR},	
		#{GMBR_GMB_LAYER},	
		#{GMBR_GMB_ROOM},	
		#{GMBR_ENVIRONMENT},	
		#{GMBR_SANITATION},	
		#{GMBR_SAFE},	
		#{GMBR_EQUIPMENT},	
		#{GMBR_CPHOTO},	
		#{GMBR_PHOTOA},	
		#{GMBR_PHOTOB},	
		#{GMBR_PHOTOC},	
		#{GMBR_PHOTOD},	
		#{GMBR_DESC},	
		#{GMBR_CTIME},	
		#{GMBR_UTIME},	
		#{GMBR_GMU_ID},	
		#{GMBRESULT_ID}
	</sql>
	
	<!-- 新增-->
	<insert id="save" parameterType="pd">
		insert into 
	<include refid="tableName"></include>
		(
	<include refid="Field"></include>
		) values (
	<include refid="FieldValue"></include>
		)
	</insert>
	
	<!-- 删除-->
	<delete id="delete" parameterType="pd">
		delete from
		<include refid="tableName"></include>
		where 
			GMBRESULT_ID = #{GMBRESULT_ID}
	</delete>
	
	<!-- 修改 -->
	<update id="edit" parameterType="pd">
		update
		<include refid="tableName"></include>
		set 
			GMBR_GMB_ID = #{GMBR_GMB_ID},
			GMBR_ENVIRONMENT = #{GMBR_ENVIRONMENT},
			GMBR_SANITATION = #{GMBR_SANITATION},
			GMBR_SAFE = #{GMBR_SAFE},
			GMBR_EQUIPMENT = #{GMBR_EQUIPMENT},
			GMBR_CPHOTO = #{GMBR_CPHOTO},
			GMBR_PHOTOA = #{GMBR_PHOTOA},
			GMBR_PHOTOB = #{GMBR_PHOTOB},
			GMBR_PHOTOC = #{GMBR_PHOTOC},
			GMBR_PHOTOD = #{GMBR_PHOTOD},
			GMBR_DESC = #{GMBR_DESC},
			GMBR_GMU_ID = #{GMBR_GMU_ID},
		GMBRESULT_ID = GMBRESULT_ID
		where 
		GMBRESULT_ID = #{GMBRESULT_ID}
	</update>
	
	<!-- 通过ID获取数据 -->
	<select id="findById" parameterType="pd" resultType="pd">
		select 
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 
			GMBRESULT_ID = #{GMBRESULT_ID}
	</select>
	
	<!-- 列表 -->
	<select id="datalistPage" parameterType="page" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
		where 1=1
		<if test="pd.keywords!= null and pd.keywords != ''"><!-- 关键词检索 -->
			and
				(
				<!--	根据需求自己加检索条件
					字段1 LIKE CONCAT(CONCAT('%', #{pd.keywords}),'%')
					 or 
					字段2 LIKE CONCAT(CONCAT('%', #{pd.keywords}),'%') 
				-->
				)
		</if>
	</select>
	
	<!-- 列表(全部) -->
	<select id="listAll" parameterType="pd" resultType="pd">
		select
		<include refid="Field"></include>
		from 
		<include refid="tableName"></include>
	</select>
	
	<!-- 批量删除 -->
	<delete id="deleteAll" parameterType="String">
		delete from
		<include refid="tableName"></include>
		where 
			GMBRESULT_ID in
		<foreach item="item" index="index" collection="array" open="(" separator="," close=")">
                 #{item}
		</foreach>
	</delete>
	
</mapper>